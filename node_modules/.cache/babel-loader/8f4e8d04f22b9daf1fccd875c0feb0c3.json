   async mounted(){\r\n      await $('#funcionarios').DataTable()\r\n    },\r\n    watch: {\r\n      funcionarios(val) {\r\n        if(this.pesta√±a === 'funcionarios'){\r\n          $('#funcionarios').DataTable().destroy();\r\n          this.$nextTick(()=> {\r\n            $('#funcionarios').DataTable()\r\n          });\r\n        }\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    body{\r\n        background-color: #eee;\r\n    }\r\n\r\n    .texto{\r\n        text-align: center;\r\n    }\r\n\r\n    .imagen{\r\n        width: 100%;\r\n    }\r\n    .boton{\r\n        margin: 20px;\r\n        width: 90%;\r\n        border-radius: 12px !important;\r\n        border-color: black !important;\r\n    }\r\n</style>"]}]}