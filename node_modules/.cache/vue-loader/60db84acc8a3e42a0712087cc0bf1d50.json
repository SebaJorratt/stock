n                            <select class=\"form-control\" v-model=\"$v.tipo.$model\">\r\n                            <option disabled value=\"\">Tipo de usuario</option>\r\n                                <option value=\"1\">Administrador</option>\r\n                                <option value=\"0\">Normal</option>\r\n                            </select>\r\n                        </div>\r\n                      </b-col>\r\n                    </b-row><br>\r\n                    <b-row>\r\n                        <b-container>\r\n                          <b-button @click=\"agregarUsuario()\" class=\"btn-success botonmostrar\">Agregar</b-button>\r\n                        </b-container>\r\n                    </b-row>\r\n                </div>\r\n              </div>\r\n            </b-container>\r\n          </div>\r\n        </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport navbar from \"../components/navbar.vue\";\r\nimport { required, email, minLength } from \"vuelidate/lib/validators\";\r\nexport default {\r\n  name: \"about\",\r\n  components: {\r\n    navbar,\r\n  },\r\n  data() {\r\n      return {\r\n        nombre: '',\r\n        email: '',\r\n        password: '',\r\n        tipo: ''\r\n      }\r\n    },\r\n    validations:{\r\n      nombre:{required, minLength: minLength(10)},\r\n      email:{required,email},\r\n      password:{required, minLength: minLength(6)},\r\n      tipo:{required}\r\n    },\r\n    created(){\r\n\r\n    },\r\n    methods: {\r\n      agregarUsuario(){\r\n        console.log('enviar usuario agregado')\r\n      }\r\n    }\r\n};\r\n</script>\r\n\r\n<style>\r\n  .config{\r\n    padding: 30px;\r\n  }\r\n\r\n  body{\r\n    background-color: #eee;\r\n  }\r\n</style>"]}]}